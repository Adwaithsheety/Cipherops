    <script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
	<script>
    $.noConflict();

    if (typeof Prototype !== "undefined" && Prototype.BrowserFeatures.ElementExtensions)
    {
        var disablePrototypeJS = function (method, pluginsToDisable)
        {
            var handler = function (event)
            {
                event.target[method] = undefined;
                setTimeout(function ()
                {
                    delete event.target[method];
                }, 0);
            };

            pluginsToDisable.each(function (plugin)
            {
                jQuery(window).on(method + '.bs.' + plugin, handler);
            });
        },

        pluginsToDisable = ['collapse', 'dropdown', 'modal', 'tooltip', 'popover', 'tab','collect'];
        disablePrototypeJS('show', pluginsToDisable);
        disablePrototypeJS('hide', pluginsToDisable);
    }
	</script>
	<script>
jQuery( document ).ready(function() {

	hljs.initHighlightingOnLoad();
	hljs.configure({useBR: true});

	jQuery('span.code').each(function(i, block) {
	  hljs.highlightBlock(block);
	});

    if (1==0 && window.canRunAds === true) {
        jQuery('#blocker').hide();
    } else { jQuery('#blocker').show(); }
});

	</script>
